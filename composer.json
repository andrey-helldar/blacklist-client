{
    "name": "andrey-helldar/blacklist-client",
    "description": "The blacklist client package for connecting to the Blacklist Server",
    "type": "library",
    "license": "MIT",
    "keywords": ["spam", "blacklist", "blacklist", "antispam", "anti-spam", "laravel", "blacklist"],
    "support": {
        "issues": "https://github.com/andrey-helldar/blacklist-client/issues",
        "source": "https://github.com/andrey-helldar/blacklist-client"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "andrey-helldar/blacklist-core": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^4.0",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "suggest": {
        "andrey-helldar/blacklist-server": "The blacklist server package"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\BlacklistClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\BlacklistClient\\ServiceProvider"
            ]
        }
    }
}
